      title: \"This is an error message\",\r\n      content: \"Please connect your Wallet\",\r\n    });\r\n  };\r\n\r\n  const connectWallet = () => {\r\n    web3.connect(fetchWeb3Data, showWeb3ErrorModal, false);\r\n  };\r\n\r\n  const fetchWeb3Data = async () => {\r\n    const { address } = web3.state;\r\n    setWallet(address);\r\n    setWalletAddress(address);\r\n    const ethBalance = getEthFromWei(await web3.getEthBalance(address));\r\n    setEthBalance(ethBalance);\r\n  };\r\n\r\n  const showDrawer = () => {\r\n    setVisibility(!visible);\r\n  };\r\n\r\n  const onClose = () => {\r\n    setVisibility(!visible);\r\n  };\r\n\r\n  const MenuItem = () => {\r\n    return (\r\n      <Fragment>\r\n        <S.CustomNavLinkSmall style={{ width: \"220px\" }}>\r\n          {wallet ? (\r\n            <S.WalletContainer>\r\n              <Jazzicon diameter={34} seed={jsNumberForAddress(wallet)} />\r\n              <a\r\n                style={{ color: \"white\" }}\r\n                href={`${getNetworkExplorer()}/address/${wallet}`}\r\n                target=\"_blank\"\r\n              >\r\n                {shortenAddress(wallet)}\r\n              </a>\r\n              <S.NotHidden>\r\n                <span style={{ color: \"white\" }}>({numberWithCommas(formatNumber(ethBalance))} ETH)</span>\r\n              </S.NotHidden>\r\n            </S.WalletContainer>\r\n          ) : (\r\n            <Link to=\"/\">\r\n              <Button color=\"white\" onClick={connectWallet}>\r\n                {t(\"Connect wallet\")}\r\n              </Button>\r\n            </Link>\r\n          )}\r\n        </S.CustomNavLinkSmall>\r\n      </Fragment>\r\n    );\r\n  };\r\n\r\n  return (\r\n    <Affix offsetTop={top}>\r\n      <S.Header>\r\n        <S.Container>\r\n          <Row type=\"flex\" justify=\"space-between\" gutter={20}>\r\n            <Col>\r\n              <S.LogoContainer to=\"/\">\r\n                <img src={SwLogo} width=\"140\"/>\r\n              </S.LogoContainer>\r\n            </Col>\r\n            <S.NotHidden>\r\n              <Col\r\n                style={{\r\n                  display: \"flex\",\r\n                  alignItems: \"center\",\r\n                  marginTop: \"0.625rem\",\r\n                }}\r\n              >\r\n                <MenuItem />\r\n              </Col>\r\n            </S.NotHidden>\r\n            <S.Burger onClick={showDrawer}>\r\n              <S.Outline />\r\n            </S.Burger>\r\n          </Row>\r\n          <CSSTransition\r\n            in={!isSmallScreen || isNavVisible}\r\n            timeout={350}\r\n            classNames=\"NavAnimation\"\r\n            unmountOnExit\r\n          >\r\n            <Drawer closable={false} visible={visible} onClose={onClose}>\r\n              <Col style={{ marginBottom: \"2.5rem\" }}>\r\n                <S.Label onClick={onClose}>\r\n                  <Col span={12}>\r\n                    <S.Menu>Menu</S.Menu>\r\n                  </Col>\r\n                  <Col span={12}>\r\n                    <S.Outline padding=\"true\" />\r\n                  </Col>\r\n                </S.Label>\r\n              </Col>\r\n              <MenuItem />\r\n            </Drawer>\r\n          </CSSTransition>\r\n        </S.Container>\r\n      </S.Header>\r\n    </Affix>\r\n  );\r\n};\r\n\r\nexport default Header;\r\n","/Users/ararat/Desktop/SW-Capital/git/dca-vaults/ui/src/web3Enabled.js",[],"/Users/ararat/Desktop/SW-Capital/git/dca-vaults/ui/src/components/Header/styles.js",[],"/Users/ararat/Desktop/SW-Capital/git/dca-vaults/ui/src/components/Home/EmptyMessage.js",[],["94","95"],"/Users/ararat/Desktop/SW-Capital/git/dca-vaults/ui/src/components/Home/Tabs.js",[],"/Users/ararat/Desktop/SW-Capital/git/dca-vaults/ui/src/helpers/index.js",[],"/Users/ararat/Desktop/SW-Capital/git/dca-vaults/ui/src/common/Container/index.js",[],"/Users/ararat/Desktop/SW-Capital/git/dca-vaults/ui/src/common/SvgIcon/index.js",[],"/Users/ararat/Desktop/SW-Capital/git/dca-vaults/ui/src/common/Bu